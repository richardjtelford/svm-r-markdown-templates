@InBook{Lamb2002,
  Title                    = {Origin and isotopic composition of aragonite laminae in an {Ethiopian} crater lake},
  Author                   = {Lamb, Henry and Kebede, Seifu and Leng, Melanie and Ricketts, Douglas and Telford, Richard and Umer, Mohammed},
  Editor                   = {Odada, Eric O. and Olago, Daniel O.},
