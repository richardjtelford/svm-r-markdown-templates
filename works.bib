@Article{Telford1999,
  Title                    = {Diatom-derived palaeoconductivity estimates for Lake Awassa, Ethiopia: evidence for pulsed inflows of saline groundwater},
  Author                   = {Telford, Richard J. and Lamb, Henry F. and Umer Mohammed, Mohammed},
